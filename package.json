    "type": "git",
    "url": "git+https://github.com/selfage/bundler_cli.git"
  },
  "keywords": [
    "Build",
    "Compile",
    "Asset",
    "Image",
    "Browserify",
    "Uglify",
    "Bundle",
    "Runtime",
    "Puppeteer",
    "Browser",
    "TypeScript"
  ],
  "author": "Yuxuan Xie <teststaybaka@gmail.com> (http://www.yuxuanxie.com)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/selfage/bundler_cli/issues"
  },
  "homepage": "https://github.com/selfage/bundler_cli#readme",
  "bin": {
    "bundage": "./main.js"
  },
  "files": [
