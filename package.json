    "@selfage/web_app_base_dir": "^1.0.0",
    "@types/browserify": "^12.0.36",
    "@types/express": "^4.17.11",
    "@types/node": "^16.3.3",
    "@types/uglify-js": "^3.13.1",
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@selfage/cli": "^4.0.0",
    "@selfage/message": "^3.1.7",
    "@selfage/puppeteer_test_executor": "^3.0.2",
    "browserify": "^17.0.0",
